{
    "title": "费曼物理学讲义学习",
    "description": "费曼物理学讲义学习。",
    "author": "程磊根",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "links": {
        "sidebar": {
            "费曼物理学讲义学习": "https://github.com/clgsnail/physics_learning_gitbook.git"
        }
    },
    "plugins": [
         "-lunr", 
         "-search",
         "-sharing",
         "-fontsettings",
         "highlight",
         "livereload",
         "search-pro",
         "expandable-chapters-small",
         "splitter",
         "back-to-top-button",
         "github",
         "hide-element",
         "code",
         "page-toc-button",
         "tbfed-pagefooter",
         "katex"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/clgsnail/physics_learning_gitbook.git"
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
       "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy 版权信息",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}